/include/ "system-conf.dtsi"
/ {
	chosen {
		bootargs = " earlycon console=ttyPS0,115200 clk_ignore_unused root=/dev/ram0 rw";
		stdout-path = "serial0:115200n8";
	};
};
&i2c1 {
	i2c-mux@74 {
		i2c_eeprom: i2c@0 {
			eeprom: eeprom@54 {
				#address-cells = <1>;
				#size-cells = <1>;
				compatible = "atmel,24c128";
				reg = <0x54>;
				mac_address: mac-address@23 {
					reg = <0x23 6>;
				};
			};
		};
		i2c_si570_mgt: i2c@3 {
			si570_2: clock-generator@5d { /* USER MGT SI570 - u48 */
				clock-frequency = <156250000>;
			};
		};
	};
};
&gem3 {
	nvmem-cells = <&mac_address>;
	nvmem-cell-names = "mac-address";
	label = "eth0";
};
/ {
	axi_stream_dma_0@b0000000 {
		compatible = "axi_stream_dma";
		reg = <0x0 0xb0000000 0x0 0x10000>;
		interrupts = <0 89 4>;
		interrupt-parent = <&gic>;
	};
	usp_rf_data_converter@490000000 {
		compatible = "xlnx,usp-rf-data-converter-2.6";
		num-insts = <0x1>;
		param-list = [ ];
		reg = <0x4 0x90000000 0x0 0x40000>;
	};
};

