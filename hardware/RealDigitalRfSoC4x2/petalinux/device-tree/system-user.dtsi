/include/ "system-conf.dtsi"
#include <dt-bindings/phy/phy.h>
/ {
	chosen {
		bootargs = " earlycon console=ttyPS0,115200 clk_ignore_unused root=/dev/ram0 rw net.ifnames=0 biosdevname=0 cma=1536M";
		stdout-path = "serial0:115200n8";
	};

	reserved-memory {
		#address-cells = <2>;
		#size-cells = <2>;
		ranges;

		cma_reserved: cma@0 {
			compatible = "shared-dma-pool";
			reusable;
			reg = <0x0 0x80000000 0x0 0x60000000>; /* 1.5 GB reserved at 2GB boundary */
			alignment = <0 0x1000>;
			no-map;
		};
	};
};
&sdhci0 {
	no-1-8-v;
	disable-wp;
};
&spi0 {
	status = "disabled";
};
&spi1 {
	spidev@0 {
		compatible = "rohm,dh2228fv";
		reg = <0>;
		spi-max-frequency = <500000>;
	};
};
&gem1 {
	status = "okay";
	phy-handle = <&phy0>;
	phy-mode = "rgmii-id";
	nvmem-cells = <&mac_address>;
	nvmem-cell-names = "mac-address";
	/* pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_gem1_default>; */
	phy0: phy@f {
		reg = <0xf>;
		ti,rx-internal-delay = <0x8>;
		ti,tx-internal-delay = <0xa>;
		ti,fifo-depth = <0x1>;
		ti,dp83867-rxctrl-strap-quirk;
	};
};
&i2c1 {
	eeprom: eeprom@58 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "atmel,24mac402";
		pagesize = <256>;
		read-only;
		reg = <0x58>;
		mac_address: mac-address@9A {
			reg = <0x9A 6>;
		};
	};
};
/ {
	axi_stream_dma_0@b0000000 {
		compatible = "axi_stream_dma";
		reg = <0x0 0xb0000000 0x0 0x10000>;
		interrupts = <0 89 4>;
		interrupt-parent = <&gic>;
	};
	usp_rf_data_converter@490000000 {
		compatible = "xlnx,usp-rf-data-converter-2.6";
		num-insts = <0x1>;
		param-list = [ ];
		reg = <0x4 0x90000000 0x0 0x40000>;
	};
};
